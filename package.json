{
  "name": "pauyographiques",
  "version": "1.0.0",
  "private": true,
  "description": "Pauyographiques – Plateforme créative mêlant boutique, streaming en direct et média artistique.",
  "main": "src/main.jsx",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,md}\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  },
  "dependencies": {
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.28",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "prettier": "^3.1.0",
    "cssnano": "^6.0.1"
  },
  "keywords": [
    "Pauyographiques",
    "React",
    "TailwindCSS",
    "PWA",
    "Streaming",
    "Boutique",
    "Créatif",
    "Media"
  ],
  "author": "Pauyo Nephtalie",
  "license": "MIT"
}
